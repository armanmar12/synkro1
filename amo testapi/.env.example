# amoCRM account domain, e.g. example.amocrm.ru
AMO_DOMAIN=

# Local OAuth receiver bind
AMO_LOCAL_HOST=127.0.0.1
AMO_LOCAL_PORT=8787

# OAuth2 app credentials
AMO_CLIENT_ID=
AMO_CLIENT_SECRET=
AMO_REDIRECT_URI=

# Fill one of the following to bootstrap tokens:
# 1) Authorization code from redirect callback
AMO_AUTH_CODE=

# 2) Or existing refresh token
AMO_REFRESH_TOKEN=

# Optional: pre-existing access token
AMO_ACCESS_TOKEN=
